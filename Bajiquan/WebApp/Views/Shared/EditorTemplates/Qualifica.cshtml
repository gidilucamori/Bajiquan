@{
    var values = Enum.GetValues(typeof(Bajiquan.Database.Qualifica)).Cast<Bajiquan.Database.Qualifica>();

    IEnumerable<SelectListItem> items =
        from value in values
        select new SelectListItem
        {
            Text = value.ToString(),
            Value = value.ToString(),
            Selected = (value.Equals(Bajiquan.Database.Qualifica.SocioOrdinario))
        };
}

@Html.DropDownList("", items)